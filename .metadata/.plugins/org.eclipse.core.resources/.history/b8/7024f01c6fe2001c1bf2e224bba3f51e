def codificar(cadena, clave):
    resultado=""
    for i in range(0, len(cadena)):
        if i<=len(cadena):
            if cadena[i]==i:
                resultado=resultado+clave[i]
    
    
    
    return resultado
    
print(codificar("129", "3651829470"))