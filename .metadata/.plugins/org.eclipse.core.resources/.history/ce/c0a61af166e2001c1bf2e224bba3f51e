cadena=input("Introduce la identificacion de la SS: ")
cadenaAux1=""
cadenaAux2=""
encontrado1=False
encontrado2=False
for i in range(0, len(cadena)):
    if cadena[i]=="-":
        encontrado1=True
    if encontrado1==False:
        cadenaAux1=cadenaAux1+cadena[i]
        
for i in range(0, len(cadena)):
    if encontrado2==True:
        cadenaAux2=cadenaAux2+cadena[i]
    if cadena[i]=="-":
        encontrado2=True

if encontrado1==False or encontrado2==False:
    print("No se encontr√≥ el separador")
else:
    print(cadenaAux1)
    print(cadenaAux2)




def compruebaA(cadena):
    resultado=True
    if len(cadena)<2:
        resultado=False
    else:
        for i in range(0, len(cadena)):
            ascii=ord(cadena[i])
            if ascii<65 or ascii>90:
                resultado=False
    return resultado









