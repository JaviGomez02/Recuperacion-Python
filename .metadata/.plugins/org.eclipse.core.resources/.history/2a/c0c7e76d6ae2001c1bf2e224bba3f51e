    return resultado


def esChachi(cadena):
    resultado=False
    NUMERO=4
    if len(cadena)>=8:
        cadena1=""
        cadena2=""
        for i in range(0, NUMERO):
            cadena1=cadena1+cadena[i]
        for i in range(len(cadena)-NUMERO, len(cadena)):
            cadena2=cadena2+cadena[i]
        if cadena1==cadena2:
            resultado=True
    return resultado

print(esChachi("manolo juega al balomano"))