def esChachi(cadena):
    resultado=False
    if len(cadena)>=8:
        cadena1=""
        cadena2=""
        for i in range(0, 3):
            cadena1=cadena1+cadena[i]
        for i in range(len(cadena)-4, len(cadena)):
            cadena2=cadena2+cadena[i]
    
    
    print(cadena1)
    print(cadena2)
    return resultado