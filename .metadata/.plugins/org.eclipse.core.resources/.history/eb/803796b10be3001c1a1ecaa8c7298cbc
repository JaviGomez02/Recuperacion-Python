def numCaracteres(cadena, caracter):
    contador=0
    encontrado=False
    for i in range(0, len(cadena)):
        if caracter==cadena[i]:
            encontrado=True
        if cadena[i]!=caracter and encontrado:
            contador+=1

    return contador

print(numCaracteres("hooheee", "h"))
